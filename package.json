{
  "name": "cv-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:server watch:client",
    "build": "npm-run-all --parallel build:server build:client",
    "build:server":
      "webpack --mode=production --config webpack.config.server.js",
    "build:client":
      "webpack --mode=production --config webpack.config.client.js",
    "watch:server": "nodemon ./server --watch ./server --ignore ./client",
    "watch:client":
      "webpack-dev-server --mode development --config webpack.config.client.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikg123/cv-api.git"
  },
  "author": "Erik Granbom",
  "license": "ISC",
  "homepage": "https://github.com/erikg123/cv-api#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "webpack": "^4.4.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.20.3",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
